        </trans-unit>
        <trans-unit id="b75381f00486e5b8409bb3475762882a8ac61715" translate="yes" xml:space="preserve">
          <source>parameter</source>
          <target state="translated">パラメーター</target>
        </trans-unit>
        <trans-unit id="13800272d7a85c03409344e8e130d16ae2dc138f" translate="yes" xml:space="preserve">
          <source>precision</source>
          <target state="translated">せいかく</target>
        </trans-unit>
        <trans-unit id="8ee39c657a5062a67392fce85653869d7563d3d4" translate="yes" xml:space="preserve">
          <source>precision for numbers</source>
          <target state="translated">数の精度</target>
        </trans-unit>
        <trans-unit id="26718564c5726ed4fe5b2a60349fedf6ccaeda11" translate="yes" xml:space="preserve">
          <source>source_comments</source>
          <target state="translated">ソースコメント</target>
        </trans-unit>
        <trans-unit id="768063cfdf90dea64041ede27f436b5d6cbd79fc" translate="yes" xml:space="preserve">
          <source>source_dir</source>
          <target state="translated">ソースディレクトリ</target>
        </trans-unit>
        <trans-unit id="f1c4d1fb90309eb232f69790be542e16cb92ff0c" translate="yes" xml:space="preserve">
          <source>the directory in which the plugin searchs for sass files (subdirectories are included)</source>
          <target state="translated">プラグインが sass ファイルを検索するディレクトリ (サブディレクトリも含まれます)</target>
        </trans-unit>
        <trans-unit id="5f5850ec94b0b4c2efb7663287fda826b5d72aad" translate="yes" xml:space="preserve">
          <source>the directory the compiled css files get place at</source>
          <target state="translated">コンパイルされたCSSファイルが置かれるディレクトリ</target>
        </trans-unit>
        <trans-unit id="1ed824e552a953e0d8b95336e52c9ab6fe8b725c" translate="yes" xml:space="preserve">
          <source>whether a source map should be generated</source>
          <target state="translated">ソースマップを生成するかどうか</target>
