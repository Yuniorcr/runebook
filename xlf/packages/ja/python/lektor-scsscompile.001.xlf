        <trans-unit id="70b7c3633dd27a53ae9dffc92b7e45a8687a5515" translate="yes" xml:space="preserve">
          <source>whether to add comments about source lines</source>
          <target state="translated">ソース行に関するコメントを追加するかどうか</target>
        </trans-unit>
      </group>
