          <target state="translated">インストール</target>
        </trans-unit>
        <trans-unit id="231c7a8e296d4efb70422c7f8cbefa79e9fa48e6" translate="yes" xml:space="preserve">
          <source>MoinMoin wiki configured as document control</source>
          <target state="translated">ドキュメントコントロールとして設定されたMoinMoin wiki</target>
        </trans-unit>
        <trans-unit id="1257683bacd4a5a5c34125b8b19f55fc0df722cc" translate="yes" xml:space="preserve">
