        <trans-unit id="9a71b62560f6f5ac5937b5aa841d86426d18d621" translate="yes" xml:space="preserve">
          <source>I18n support, allows use of multiple languages</source>
          <target state="translated">I18n サポート、複数の言語の使用を可能にする</target>
        </trans-unit>
        <trans-unit id="fd605d60eaa8d4a6a1c1e69b30744fc59d9f9a55" translate="yes" xml:space="preserve">
