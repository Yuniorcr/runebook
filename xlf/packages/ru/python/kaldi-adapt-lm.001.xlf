          <target state="translated">Лицензия</target>
        </trans-unit>
        <trans-unit id="014bcd654c60d5250fad2123b073b9c0ac3340f6" translate="yes" xml:space="preserve">
          <source>Links</source>
          <target state="translated">Ссылки</target>
        </trans-unit>
        <trans-unit id="321115a44857bcfa71f6f531dee309e56dda20db" translate="yes" xml:space="preserve">
          <source>MODEL=&amp;rdquo;models/kaldi-generic-en-tdnn_sp-latest&amp;rdquo;
cut -f 1 -d &amp;lsquo; &amp;lsquo; ${MODEL}/data/local/dict/lexicon.txt &amp;gt;vocab.txt</source>
          <target state="translated">МОДЕЛЬ = &quot;модели / kaldi-generic-en-tdnn_sp-latest&quot; cut -f 1 -d '' $ {МОДЕЛЬ} /data/local/dict/lexicon.txt&amp;gt; vocab.txt</target>
        </trans-unit>
        <trans-unit id="bdaf96520e362128cd44a8f3747cf6938050af9b" translate="yes" xml:space="preserve">
