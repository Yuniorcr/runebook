          <target state="translated">使用方法</target>
        </trans-unit>
        <trans-unit id="7b1855527317d0124ebeb726defa838d54e9b663" translate="yes" xml:space="preserve">
          <source>pip install -r requirements.txt</source>
          <target state="translated">pip install -r requirements.txt</target>
        </trans-unit>
        <trans-unit id="34dfcbbce96e2a26d5eb49521b956001f68a86b9" translate="yes" xml:space="preserve">
          <source>pip install aristotle</source>
          <target state="translated">pip install aristotle</target>
        </trans-unit>
      </group>
