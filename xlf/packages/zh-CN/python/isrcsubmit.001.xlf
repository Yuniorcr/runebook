        </trans-unit>
        <trans-unit id="a5bf422ca9033b573f024e714e4b41de92297a17" translate="yes" xml:space="preserve">
          <source>search for releases with the TOC of the disc</source>
          <target state="translated">搜索带有光盘TOC的版本</target>
        </trans-unit>
        <trans-unit id="e5b99ed49e60c52cfbddc71176830d4dd5961b9b" translate="yes" xml:space="preserve">
          <source>submit ISRCs</source>
          <target state="translated">提交ISRC</target>
        </trans-unit>
        <trans-unit id="72a270e91e41e6052ff2f85093d35c4dfbfca332" translate="yes" xml:space="preserve">
          <source>submit ISRCs from disc to MusicBrainz</source>
          <target state="translated">将光盘中的ISRCs提交到MusicBrainz。</target>
        </trans-unit>
        <trans-unit id="0b7a5325ff35267fb6cd6b13eec5539269f89dd3" translate="yes" xml:space="preserve">
          <source>submit discIds / TOCs</source>
          <target state="translated">提交光盘/TOCs</target>
