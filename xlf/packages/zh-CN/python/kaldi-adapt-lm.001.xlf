          <source>kaldi-adapt-lm ${MODEL} lm.arpa mymodel</source>
          <target state="translated">kaldi-adapt-lm ${MODEL}lm.arpa mymodel。</target>
        </trans-unit>
        <trans-unit id="b2941b4fc44d624a53e24f93aee3e6875988a25c" translate="yes" xml:space="preserve">
          <source>lmplz -o 4 &amp;ndash;prune 0 1 2 3 &amp;ndash;limit_vocab_file vocab.txt &amp;ndash;interpolate_unigrams 0 &amp;lt;lm.txt &amp;gt;lm.arpa</source>
