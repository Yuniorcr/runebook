        </trans-unit>
        <trans-unit id="0bb18642b70b9f8a9c12ccf39487328f306b8e19" translate="yes" xml:space="preserve">
          <source>Usage</source>
          <target state="translated">용법</target>
        </trans-unit>
        <trans-unit id="d388bc0b5c6f31121e2b071573b613a6d71e6590" translate="yes" xml:space="preserve">
          <source>mpdnotify</source>
          <target state="translated">mpdnotify</target>
        </trans-unit>
        <trans-unit id="5eec3c7734919dfb5d8a3f83df2f796846290a04" translate="yes" xml:space="preserve">
          <source>mpdnotify - get notification when changing song</source>
          <target state="translated">mpdnotify-노래 변경시 알림 받기</target>
        </trans-unit>
        <trans-unit id="7845114365ee3a416b0467007c38d20046afe6b4" translate="yes" xml:space="preserve">
          <source>python-mpd2</source>
          <target state="translated">파이썬 -mpd2</target>
        </trans-unit>
      </group>
