          <target state="translated">칼디 ASR</target>
        </trans-unit>
        <trans-unit id="3229609e15436ec51bcf00818a69a84dbc58a0c2" translate="yes" xml:space="preserve">
          <source>License</source>
          <target state="translated">특허</target>
        </trans-unit>
        <trans-unit id="014bcd654c60d5250fad2123b073b9c0ac3340f6" translate="yes" xml:space="preserve">
          <source>Links</source>
          <target state="translated">연결</target>
        </trans-unit>
        <trans-unit id="321115a44857bcfa71f6f531dee309e56dda20db" translate="yes" xml:space="preserve">
          <source>MODEL=&amp;rdquo;models/kaldi-generic-en-tdnn_sp-latest&amp;rdquo;
cut -f 1 -d &amp;lsquo; &amp;lsquo; ${MODEL}/data/local/dict/lexicon.txt &amp;gt;vocab.txt</source>
          <target state="translated">MODEL =&amp;rdquo;models / kaldi-generic-en-tdnn_sp-latest&amp;rdquo;cut -f 1 -d ''$ {MODEL} /data/local/dict/lexicon.txt&amp;gt; vocab.txt</target>
        </trans-unit>
