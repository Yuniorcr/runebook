          <source>&lt;code&gt;$ sudo apt-get install python3-dev default-libmysqlclient-dev build-essential&lt;/code&gt;  # Debian / Ubuntu</source>
          <target state="translated">&lt;code&gt;$ sudo apt-get install python3-dev default-libmysqlclient-dev build-essential&lt;/code&gt; # Debian / Ubuntu</target>
